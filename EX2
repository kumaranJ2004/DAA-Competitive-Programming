#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int arr[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

  
    int slow = arr[0];
    int fast = arr[arr[0]];
    while (slow != fast) {
        slow = arr[slow];
        fast = arr[arr[fast]];
    }

    
    fast = 0;
    while (slow != fast) {
        slow = arr[slow];
        fast = arr[fast];
    }

    printf("%d\n", slow);
    return 0;
}
